<?xml version="1.0"?>

<Feature>
  <Login>
    <InitialLogin is_button="true">
      <PttUserProfile>
        <Request>
          <tag>HTTP</tag>
          <tag>GET</tag>
          <tag>mcptt.user-profile</tag>
        </Request>
        <Response>
          <tag>HTTP</tag>
          <tag>200</tag>
        </Response>
      </PttUserProfile>
      <VideoUserProfile>
        <Request>
          <tag>HTTP</tag>
          <tag>GET</tag>
          <tag>mcvideo.user-profile</tag>
        </Request>
        <Response>
          <tag>HTTP</tag>
          <tag>200</tag>
        </Response>
      </VideoUserProfile>
      <McdataUserProfile>
        <Request>
          <tag>HTTP</tag>
          <tag>GET</tag>
          <tag>mcdata.user-profile</tag>
        </Request>
        <Response>
          <tag>HTTP</tag>
          <tag>200</tag>
        </Response>
      </McdataUserProfile>
      <GroupProfile>
        <Request>
          <tag>HTTP</tag>
          <tag>GET</tag>
          <tag>org-profile</tag>
        </Request>
        <Response>
          <tag>HTTP</tag>
          <tag>200</tag>
        </Response>
      </GroupProfile>
      <KeyInit>
        <Request>
          <tag>HTTP</tag>
          <tag>POST</tag>
          <tag>/keymanagement</tag>
          <tag>init</tag>
        </Request>
        <Response>
          <tag>HTTP</tag>
          <tag>200</tag>
        </Response>
      </KeyInit>
      <KeyProv>
        <Request>
          <tag>HTTP</tag>
          <tag>POST</tag>
          <tag>/keymanagement</tag>
          <tag>keyprov</tag>
        </Request>
        <Response>
          <tag>HTTP</tag>
          <tag>200</tag>
        </Response>
      </KeyProv>
      <InitialRegister>
        <Request>
          <tag>&lt;mcptt-access-token&gt;</tag>
          <tag>REGISTER</tag>
          <tag>SIP</tag>
          <tag>sip:</tag>
        </Request>
        <Response>
          <tag>HTTP</tag>
          <tag>200</tag>
        </Response>
      </InitialRegister>
    </InitialLogin>
  </Login>
  <CallType>
    <Audio>
      <Prearranged is_button="true">
        <INVITE>
          <Request>
            <tag>INVITE sip:(\d+)@</tag>
            <tag>&lt;session-type&gt;</tag>
            <tag>prearranged</tag>
            <tag>&lt;mcptt-request-uri&gt;</tag>
          </Request>
          <Response>
            <tag>100</tag>
            <tag>(?i)Trying</tag>
          </Response>
        </INVITE>
        <OK>
          <tag>200</tag>
          <tag>(?i)Ok</tag>
        </OK>
        <ACK>
          <tag>ACK sip:(\d+)@</tag>
        </ACK>
        <BYE>
          <Request>
            <tag>BYE sip:(\d+)@</tag>
          </Request>
          <Response>
            <tag>200</tag>
            <tag>(?i)Ok</tag>
          </Response>
        </BYE>
      </Prearranged>
    </Audio>
    <Video>
      <Prearranged is_button="true">
        <INVITE>
          <Request>
            <tag>INVITE sip:(\d+)@</tag>
            <tag>&lt;session-type&gt;</tag>
            <tag>prearranged</tag>
            <tag>&lt;mcvideo-request-uri&gt;</tag>
          </Request>
          <Response>
            <tag>100</tag>
            <tag>(?i)Trying</tag>
          </Response>
        </INVITE>
        <OK>
          <tag>200</tag>
          <tag>(?i)Ok</tag>
        </OK>
        <ACK>
          <tag>ACK sip:(\d+)@</tag>
        </ACK>
        <BYE>
          <Request>
            <tag>BYE sip:(\d+)@</tag>
          </Request>
          <Response>
            <tag>200</tag>
            <tag>(?i)Ok</tag>
          </Response>
        </BYE>
      </Prearranged>
    </Video>
  </CallType>
</Feature>
